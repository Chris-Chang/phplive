					"documentation": "application/index/controller/\n首页控制器\n\nnamespace app\\index\\controller;\n\nuse app\\index\\model\\Type;\nuse app\\index\\model\\Usersuggest;\nuse think\\Cache;\nuse think\\Config;\nuse think\\Controller;\nuse think\\Db;\nuse think\\Request;\nuse think\\Session;",
